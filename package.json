{
  "name": "mentor-mentee-matching-platform",
  "version": "1.0.0",
  "description": "멘토-멘티 매칭 플랫폼 - 멘토와 멘티를 연결하는 웹 애플리케이션",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npx http-server . -p 3000 -c-1",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run start:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mentor",
    "mentee",
    "matching",
    "platform",
    "nodejs",
    "express",
    "javascript",
    "fullstack"
  ],
  "author": "Developer",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  }
}
